{
  "name": "fatrace-parser",
  "version": "1.0.0",
  "description": "Parse and summarize fatrace output",
  "repository": "https://github.com/CommanderRedYT/fatrace-parser.git",
  "author": "CommanderRedYT <ccommanderred@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --splitting",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "start": "tsup --watch --onSuccess 'node dist/index.js input/fatrace.txt' --silent",
    "prepublish": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.16",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-mock-server": "^0.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "argparse": "^2.0.1"
  }
}
